      <MenuBar prefHeight="25.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleClose" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Booking">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleBookingSearch" text="Search" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Horse">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#handleHorseSearch" text="Search" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
